{
  "session": {
    "enabled": true,
    "client": "cookie",
    "key": "strapi.sid",
    "prefix": "strapi:sess:",
    "secretKeys": ["${process.env.SECRET_KEY_1}", "${process.env.SECRET_KEY_2}"],
    "httpOnly": true,
    "maxAge": 86400000,
    "overwrite": true,
    "signed": false,
    "rolling": false
  },
  "logger": {
    "level": "debug",
    "exposeInContext": true,
    "requests": true
  },
  "parser": {
    "enabled": true,
    "multipart": true,
    "formLimit": "128mb",
    "jsonLimit": "256mb"
  }
}
