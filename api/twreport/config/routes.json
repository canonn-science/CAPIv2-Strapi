{
  "routes": [
    {
      "method": "GET",
      "path": "/twreports",
      "handler": "Twreport.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/twreports/count",
      "handler": "Twreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/twreports/:id",
      "handler": "Twreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/twreports",
      "handler": "Twreport.create",
      "config": {
        "policies": [
          "global.blockClient",
          "global.blockCMDR",
          "global.reportValidation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/twreports/:id",
      "handler": "Twreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/twreports/:id",
      "handler": "Twreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
