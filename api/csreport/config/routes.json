{
  "routes": [
    {
      "method": "GET",
      "path": "/csreports",
      "handler": "Csreport.find",
      "config": {
        "policies": ["global::contentrange"]
      }
    },
    {
      "method": "GET",
      "path": "/csreports/count",
      "handler": "Csreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/csreports/:id",
      "handler": "Csreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/csreports",
      "handler": "Csreport.create",
      "config": {
        "policies": [
          "global::blockClient",
          "global::blockCMDR",
          "global::reportValidation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/csreports/:id",
      "handler": "Csreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/csreports/:id",
      "handler": "Csreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
