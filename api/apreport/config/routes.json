{
  "routes": [
    {
      "method": "GET",
      "path": "/apreports",
      "handler": "Apreport.find",
      "config": {
        "policies": ["global::contentRange"]
      }
    },
    {
      "method": "GET",
      "path": "/apreports/count",
      "handler": "Apreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/apreports/:id",
      "handler": "Apreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/apreports",
      "handler": "Apreport.create",
      "config": {
        "policies": [
          "global::blockClient",
          "global::blockCMDR",
          "global::reportValidation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/apreports/:id",
      "handler": "Apreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/apreports/:id",
      "handler": "Apreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
