{
  "connection": "default",
  "collectionName": "tbsites",
  "info": {
    "name": "tbsite",
    "description": "Thargoid Barnacle sites"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "location": {
      "model": "location",
      "via": "tbSite"
    },
    "siteID": {
      "unique": true,
      "min": 1,
      "type": "integer",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "type": {
      "model": "tbtype"
    },
    "cycle": {
      "model": "tbcycle"
    },
    "defense": {
      "model": "tbdefense"
    },
    "count": {
      "min": 1,
      "type": "integer"
    },
    "comment": {
      "type": "text"
    },
    "verified": {
      "type": "boolean"
    },
    "visible": {
      "default": true,
      "type": "boolean"
    },
    "discoveredBy": {
      "model": "cmdr"
    }
  }
}
