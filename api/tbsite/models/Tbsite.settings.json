{
  "connection": "default",
  "collectionName": "tbsites",
  "info": {
    "name": "TB Site"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "system": {
      "model": "system",
      "via": "tbsites"
    },
    "body": {
      "model": "body",
      "via": "tbsites"
    },
    "siteID": {
      "unique": true,
      "min": 1,
      "type": "integer",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "type": {
      "model": "tbtype"
    },
    "subtype": {
      "model": "tbsubtype"
    },
    "frontierID": {
      "type": "integer"
    },
    "cycle": {
      "model": "tbcycle"
    },
    "comment": {
      "type": "text"
    },
    "verified": {
      "type": "boolean"
    },
    "visible": {
      "default": true,
      "type": "boolean"
    },
    "discoveredBy": {
      "model": "cmdr"
    }
  }
}
