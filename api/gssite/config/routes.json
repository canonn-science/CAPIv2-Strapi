{
  "routes": [
    {
      "method": "GET",
      "path": "/gssites",
      "handler": "Gssite.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/gssites/count",
      "handler": "Gssite.count",
      "config": {
        "policies": []
      }
    },
    {
