      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/gssites/:id",
      "handler": "Gssite.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/gssites/:id",
      "handler": "Gssite.delete",
      "config": {
        "policies": []
      }
    }
  ]
