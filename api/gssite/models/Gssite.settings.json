{
  "connection": "default",
  "collectionName": "gssites",
  "info": {
    "name": "GS Site"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "system": {
      "model": "system",
      "via": "gssites"
    },
    "body": {
      "model": "body",
      "via": "gssites"
    },
    "siteID": {
      "unique": true,
      "min": 1,
      "type": "integer",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "type": {
      "model": "gstype"
    },
    "hasDatabank": {
      "type": "boolean"
    },
    "frontierID": {
      "type": "integer"
    },
    "verified": {
      "type": "boolean"
    },
    "visible": {
      "default": true,
      "type": "boolean"
    },
    "activeGroups": {
      "collection": "gsactivegroup",
      "via": "gsSite"
    },
    "activeObelisks": {
      "collection": "gsactiveobelisk",
      "via": "gsSite"
    },
    "discoveredBy": {
      "model": "cmdr"
    }
  }
}
