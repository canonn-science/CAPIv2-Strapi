{
  "routes": [
    {
      "method": "GET",
      "path": "/rings",
      "handler": "Ring.find",
      "config": {
        "policies": ["global::contentrange"]
      }
    },
    {
      "method": "GET",
      "path": "/rings/count",
      "handler": "Ring.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/rings/:id",
      "handler": "Ring.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/rings",
      "handler": "Ring.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/rings/:id",
      "handler": "Ring.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/rings/:id",
      "handler": "Ring.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
