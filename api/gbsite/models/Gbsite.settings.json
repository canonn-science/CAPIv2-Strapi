{
  "connection": "default",
  "collectionName": "gbsites",
  "info": {
    "name": "gbsite",
    "description": "Guardian Beacon site"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "system": {
      "model": "system",
      "via": "gbsites"
    },
    "body": {
      "model": "body",
      "via": "gbsites"
    },
    "siteID": {
      "default": "",
      "unique": true,
      "min": 1,
      "type": "integer",
      "required": true
    },
    "gssite": {
      "model": "gssite"
    },
    "verified": {
      "default": false,
      "type": "boolean"
    },
    "visible": {
      "default": true,
      "type": "boolean"
    },
    "gbmessage": {
      "model": "gbmessage",
      "via": "gbsite"
    },
    "discoveredBy": {
      "model": "cmdr"
    }
  }
}