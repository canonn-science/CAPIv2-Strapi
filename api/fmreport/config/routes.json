{
  "routes": [
    {
      "method": "GET",
      "path": "/fmreports",
      "handler": "Fmreport.find",
      "config": {
        "policies": ["global::contentRange"]
      }
    },
    {
      "method": "GET",
      "path": "/fmreports/count",
      "handler": "Fmreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/fmreports/:id",
      "handler": "Fmreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/fmreports",
      "handler": "Fmreport.create",
      "config": {
        "policies": [
