      "path": "/fmreports/:id",
      "handler": "Fmreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/fmreports/:id",
      "handler": "Fmreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
