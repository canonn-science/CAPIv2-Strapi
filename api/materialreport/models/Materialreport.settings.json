{
  "connection": "default",
  "collectionName": "materialreports",
  "info": {
    "name": "materialreport",
    "description": "Material reporting for engineering"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "system": {
      "type": "string",
      "required": true
    },
    "coordX": {
      "type": "float",
      "required": true
    },
    "coordY": {
      "type": "float",
      "required": true
    },
    "coordZ": {
      "type": "float",
      "required": true
    },
    "body": {
      "type": "string"
    },
    "distanceFromMainStar": {
      "type": "integer"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "category": {
      "type": "string"
    },
    "journalName": {
      "type": "string",
      "required": true
    },
    "journalLocalised": {
      "type": "string"
    },
    "count": {
      "type": "integer"
    },
    "factionState": {
      "type": "string"
    },
    "collectedFrom": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "missionReward",
        "collected",
        "scanned"
      ]
    },
    "isBeta": {
      "default": false,
      "type": "boolean"
    },
    "clientVersion": {
      "type": "string",
      "required": true
    }
  }
}
