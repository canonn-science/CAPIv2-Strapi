{
  "routes": [
    {
      "method": "GET",
      "path": "/tssites",
      "handler": "Tssite.find",
      "config": {
        "policies": ["global::contentRange"]
      }
    },
    {
      "method": "GET",
      "path": "/tssites/count",
      "handler": "Tssite.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/tssites/:id",
      "handler": "Tssite.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/tssites",
      "handler": "Tssite.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/tssites/:id",
      "handler": "Tssite.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/tssites/:id",
      "handler": "Tssite.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
