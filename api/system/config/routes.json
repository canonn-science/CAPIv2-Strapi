{
  "routes": [
    {
      "method": "GET",
      "path": "/systems",
      "handler": "System.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/systems/count",
      "handler": "System.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/systems/:id",
      "handler": "System.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/systems",
      "handler": "System.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/systems/:id",
      "handler": "System.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/systems/:id",
      "handler": "System.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
