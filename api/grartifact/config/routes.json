        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/grartifacts",
      "handler": "Grartifact.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/grartifacts/:id",
      "handler": "Grartifact.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/grartifacts/:id",
      "handler": "Grartifact.delete",
      "config": {
        "policies": []
      }
    }
  ]
