{
  "routes": [
    {
      "method": "GET",
      "path": "/grartifacts",
      "handler": "Grartifact.find",
      "config": {
        "policies": ["global::contentRange"]
      }
    },
    {
      "method": "GET",
      "path": "/grartifacts/count",
      "handler": "Grartifact.count",
      "config": {
        "policies": []
      }
    },
    {
