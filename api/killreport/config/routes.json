{
  "routes": [
    {
      "method": "GET",
      "path": "/killreports",
      "handler": "Killreport.find",
      "config": {
        "policies": ["global::content-range"]
      }
    },
    {
      "method": "GET",
      "path": "/killreports/count",
      "handler": "Killreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/killreports/:id",
      "handler": "Killreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/killreports",
      "handler": "Killreport.create",
      "config": {
        "policies": ["global::block-client", "global::block-cmdr"]
      }
    },
    {
      "method": "PUT",
      "path": "/killreports/:id",
      "handler": "Killreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/killreports/:id",
      "handler": "Killreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
