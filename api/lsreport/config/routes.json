{
  "routes": [
    {
      "method": "GET",
      "path": "/lsreports",
      "handler": "Lsreport.find",
      "config": {
        "policies": ["global::contentRange"]
      }
    },
    {
      "method": "GET",
      "path": "/lsreports/count",
      "handler": "Lsreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/lsreports/:id",
      "handler": "Lsreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/lsreports",
      "handler": "Lsreport.create",
      "config": {
        "policies": [
          "global::blockClient",
          "global::blockCMDR",
          "global::reportValidation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/lsreports/:id",
      "handler": "Lsreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/lsreports/:id",
      "handler": "Lsreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
