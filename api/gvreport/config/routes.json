{
  "routes": [
    {
      "method": "GET",
      "path": "/gvreports",
      "handler": "Gvreport.find",
      "config": {
        "policies": ["global::contentrange"]
      }
    },
    {
      "method": "GET",
      "path": "/gvreports/count",
      "handler": "Gvreport.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/gvreports/:id",
      "handler": "Gvreport.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/gvreports",
      "handler": "Gvreport.create",
      "config": {
        "policies": [
          "global::blockClient",
          "global::blockCMDR",
          "global::reportValidation"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/gvreports/:id",
      "handler": "Gvreport.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/gvreports/:id",
      "handler": "Gvreport.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
