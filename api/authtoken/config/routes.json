    },
    {
      "method": "PUT",
      "path": "/authtokens/:id",
      "handler": "Authtoken.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/authtokens/:id",
      "handler": "Authtoken.delete",
