{
  "routes": [
    {
      "method": "GET",
      "path": "/authtokens",
      "handler": "Authtoken.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/authtokens/count",
      "handler": "Authtoken.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/authtokens/:id",
