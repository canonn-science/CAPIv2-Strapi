{
  "connection": "default",
  "collectionName": "gsreports",
  "info": {
    "name": "gsreport",
    "description": "Reporting for Guardian Structures"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": "Need to update type to match current types"
  },
  "attributes": {
    "systemName": {
      "type": "string",
      "required": true
    },
    "bodyName": {
      "type": "string",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "type": {
      "enum": [
        "Lacrosse",
        "Crossroads",
        "Fistbump",
        "Hammerbot",
        "Bear",
        "Bowl",
        "Turtle"
      ],
      "type": "enumeration",
      "required": true
    },
    "hasDatabank": {
      "type": "boolean"
    },
    "cmdrName": {
      "type": "string",
      "required": true
    },
    "cmdrComment": {
      "type": "text"
    },
    "isBeta": {
      "type": "boolean"
    },
    "screenshot": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "reportStatus": {
      "default": "pending",
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        " updated",
        " accepted",
        " declined",
        " issue"
      ]
    },
    "reportComment": {
      "type": "text"
    },
    "voteCount": {
      "type": "integer"
    },
    "added": {
      "type": "boolean"
    },
    "site": {
      "model": "gssite"
    },
    "gsobeliskgroupreport": {
      "collection": "gsobeliskgroupreport",
      "via": "gsreport"
    },
    "gsobeliskreport": {
      "collection": "gsobeliskreport",
      "via": "gsreport"
    }
  }
}
